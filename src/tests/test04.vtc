varnishtest "Test vmod 3Scale"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import threescale from "${vmod_topbuild}/src/.libs/libvmod_threescale.so";

	sub vcl_deliver {
	
		set resp.http.res2 = threescale.send_get_request_body("localhost", "80", "/oauth_authorize.html", "");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	
	expect resp.http.res2 == "HTTP/1.1 200 OK"
}


client c1 -run

